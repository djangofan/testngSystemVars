<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="SmokeTests" parallel="classes" thread-count="5" time-out="900000" verbose="2">
	<groups>
		<run>
			<include name="smoke" />
		</run>
	</groups>
	<test name="SmokeTests">
		<classes>
            <class name="qa.test.tests.SmokeTest" />
		</classes>
	</test>
</suite>
